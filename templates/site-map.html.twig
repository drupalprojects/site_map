<?php
/**
 * @file
 * Theme implementation to display the site map.
 *
 * Available variables:
 * - $message:
 * - $rss_legend:
 * - $front_page:
 * - $blogs:
 * - $books:
 * - $menus:
 * - $faq:
 * - $taxonomys:
 * - $additional:
 *
 * @see template_preprocess()
 * @see template_preprocess_site_map()
 */
?>

<div id="site-map">
  {% if message %}
    <div class="site-map-message">
      {{ message }}
    </div>
  {% endif %}

  {% if rss_legend %}
    <div class="site-map-rss-legend">
      {{ rss_legend }}
    </div>
  {% endif %}

  {% if front_page %}
    <div class="site-map-front-page">
      {{ front_page }}
    </div>
  {% endif %}

  {% if blogs %}
    <div class="site-map-blogs">
      {{ blogs }}
    </div>
  {% endif %}

  {% if books %}
    <div class="site-map-books">
      {{ books }}
    </div>
  {% endif %}

  {% if menus %}
    <div class="site-map-menus">
      {{ menus }}
    </div>
  {% endif %}

  {% if faq %}
    <div class="site-map-faq">
      {{ faq }}
    </div>
  {% endif %}

  {% if taxonomys %}
    <div class="site-map-taxonomys">
      {{ taxonomys }}
    </div>
  {% endif %}

  {% if additional %}
    <div class="site-map-additional">
      {{ additional }}
    </div>
  {% endif %}
</div>
